{
    "builds":[
        {
            "src": "index.js",
            "use": "@now/node"
        }
    ],
    "routes" :[
        {
            "headers":{
               "Access-Control-Allow-Origin": "*",
               "Access-Control-Allow-Methods": "OPTIONS,GET,PUT,DELETE,POST",
                "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
            },
            "src": "/.*",
            "dest": "index.js"
        }
    ]
}